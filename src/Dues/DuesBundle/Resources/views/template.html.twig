<html>
    <head>
        {% stylesheets 'bundles/dues/css/*' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        {% javascripts '@DuesBundle/Resources/public/js/*' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        <title>
            {% block title %}
                Dues
            {% endblock %}
        </title>
        {%block head %}
        {% endblock %}
    </head>
    <body>
        {%block body %}
            {%block menu %}
                {% include "DuesBundle::menu.html.twig" %}
            {% endblock %}
            <div class="content">
            {% block content %}
            {% endblock %}
            </div>
        {% endblock %}
    </body>
</html>